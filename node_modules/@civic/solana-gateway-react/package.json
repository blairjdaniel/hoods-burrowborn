{
  "name": "@civic/solana-gateway-react",
  "version": "1.2.4",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": "git@github.com:civicteam/ociv-gatekeeper.git",
  "description": "Civic Pass Solana React Component",
  "dependencies": {
    "@civic/gateway-client-core": "1.1.3",
    "@civic/gateway-client-react": "1.2.4",
    "@civic/solana-gateway-chain-client": "0.0.5",
    "@civic/prove-solana-wallet": "0.4.3",
    "@civic/solana-gateway-ts": "0.13.0",
    "@solana/web3.js": "^1.95.3",
    "prop-types": "^15.8.1",
    "ramda": "^0.28.0",
    "uuid": "^9.0.1",
    "zustand": "^4.5.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint -c ../.eslintrc.json './src/**/*.ts' --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "clean": "rm -rf ./dist",
    "build": "yarn clean && ts-node -P ../tsconfig.json ../generateVersion ./package.json ./src/version.ts && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --noEmit false --outDir dist/esm",
    "build:cjs": "tsc --noEmit false --module commonjs --outDir dist/cjs",
    "prepare": "yarn build",
    "script": "ts-node -O '{\"module\":\"commonjs\"}' --transpile-only --files"
  },
  "devDependencies": {
    "typescript": "^4.6.4"
  }
}
